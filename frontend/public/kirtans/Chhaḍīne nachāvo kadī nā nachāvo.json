{
  "song_lyrics": [
    "Chhaḍīne nachāvo, kadī nā nachāvo,",
    "Daḍāne uchhāḷo, kadī nā uchhāḷo,",
    "Juo nā juo ke haso nā haso paṇ,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Kadī sevako je kahe tem karatā,",
    "Kadī lākh kahevā chhatā nā ja karatā,",
    "Batāve kaīk ne juo kyāk anyam,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Prasādī jamo chho, kadī āpmeḷe,",
    "Kadī sevakonā jatā yatna eḷe,",
    "Madhur ho prasādī chhatā rāg shūnyam,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Batāvo kadī jāgṛutine kadī to,",
    "Sabhāmā shayanne sukhethī graho chho,",
    "Avasthā guṇothī pare āp nityam,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Pratibhāv āpo kadī hāsya laṭake,",
    "Kadī nā ja āpo hajāro prayatne,",
    "Parādhīn lāgo kadī to svatantram,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Kadī oḷakhīne madhur hāsya relo,",
    "Kadī vismaraṇno akaḷ khel khelo,",
    "Ubhay chhe amāru smṛutidhan ananyam,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Ame je vichāryu karo dhūḷ-dhāṇī,",
    "Kadī je na dhāryu dīyo sukh lhāṇī,",
    "Faro khūrashīmā feravo sarva tantram,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Juo chho badhānu rahī sarvamāhī,",
    "Karo chho kasoṭī vasī anyamāhī,",
    "Swayam Brahma Akṣhar dīso chho manuṣhyam,",
    "Pramukhjī tamāru sakal divya divyam.",
    "",
    "Tīrodhān thaīne pragaṭ chho sadāye,",
    "Rahyā chho Mahant swarūpe kṛupāe,",
    "Dīse rūp anyam swarūpe ananyam,",
    "Pramukhjī tamāru sakal divya divyam."
  ],
