{
  "song_lyrics": [
    "Sādhu em oḷkhāy,",
    "Harīras pīe saune pāy,",
    "Sāchā sādhu em oḷkhāy jī...",
    "Sāgar jevā dilḍā jenā,",
    "kedī nav chhalkāy jī;",
    "Jhernā ghuṭḍāne jīrvī jāṇe,",
    "pachhī amrut saune pāy... 1",
    "Chhīpmāthī chhuṭā paḍi motī,",
    "pārakh pāse jāy jī;",
    "Vīndhṇu laī ene vīnḍhī nākhe,",
    "pachhī mugaṭ māhī maḍhāy... 2",
    "Pathrā sange hīro paḍyo,",
    "enī kimmat nav ankāy jī;",
    "Jhaverī ene mathe julam kare,",
    "pachhī khame ghaṇnā ghay... 3",
    "Sonā jevā shuddh santo,",
    "agnimāhe orāy jī;",
    "Kasoṭī ṭāṇe ochhā na ūtre,",
    "soye ṭachnā sadāy... 4",
    "Gurupratāpe kavi kahe chhe,",
    "sang eno sukhdāyīi jī;",
    "Satsang āvo sāpḍe,",
    "to bhāgyarekhā palṭāy... 5"
  ],
