{
  "song_lyrics": [
    "Swāmī Yogīnī rūḍī joḍaḷī re,",
    "e to shobhe joḍājoḍ jo,",
    "Nīrakhīne haiyā mārā ūmaṭyā,",
    "pūryā haiyānā koḍ jo...",
    "Sant swarūpe Swāmī āviyā re,",
    "karvā nij jannā kāj jo,",
    "Kaīk pāpīne ugāriyā,",
    "sajī bhaktinā sāj jo... Swāmī 1",
    "Parchā pūryā kaī premthī re,",
    "bhāve bāndhyā mandir jo,",
    "Nīrakhī bhakto sau ānandmā,",
    "joī Swāmī sundar jo... Swāmī 2",
    "Anek jīva uddhārvā re,",
    "pragaṭyā Swāmī Bhagvant jo,",
    "Akshar sahit vahālo āviyā,",
    "sāthe mukta apār jo... Swāmī 3",
    "Dukh kāpyā kaīk bhaktanā re,",
    "dharī haiyāmā khant jo,",
    "Mandir bāndhyā ūnchā shīkhare,",
    "karyā adbhut kaī kām jo... Swāmī 4",
    "Dhanya dhanya jo evā santne re,",
    "karīe nitya praṇām jo,",
    "Karīe samāgam emno,",
    "pūre haiyānī hām jo... Swāmī 5",
    "Yogī beṭhā Akshar Derīe re,",
    "sādhe rūḍā e jog jo,",
    "Mast banīne mahāltā,",
    "tyāgī māyāṇā bhog jo... Swāmī 6",
    "Yagnapurush sam koī nahi re,",
    "jāṇe Shrījī sākshāt jo,",
    "‘Shankar’ sadā em māngto,",
    "haiye rehjo sākshāt jo... Swāmī 7"
  ],
