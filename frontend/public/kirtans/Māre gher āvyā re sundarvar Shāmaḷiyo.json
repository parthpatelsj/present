{
  "song_lyrics": [
    "Mare gher āvyā re,",
    "sundarvar Shāmaḷiyo...",
    "Harakh bharī hu Harine nīrakhu,",
    "piyu prītam pātaḷiyo... mare 1",
    "Sukhḍu devā ne man harī levā,",
    "am par aḍhaḷak ḍhaḷiyo... mare 2",
    "Jāḷavīyu joban je sāru,",
    "te avsar āj maḷiyo... mare 3",
    "Lakshmīno var lāḍakvāyo,",
    "akaḷ na jāye kaḷiyo... mare 4",
    "Āscharya vāt sejaḍīe āvyā,",
    "bol potāne paḷiyo... mare 5",
    "Muktānand Mohanvar maḷtā,",
    "khoyā dī no khag vaḷiyo... mare 6"
  ],
