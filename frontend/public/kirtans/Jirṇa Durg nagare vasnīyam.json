{
  "song_lyrics": [
    "Jirṇa Durg nagare vasanīyam,",
    "Guṇatīt muni sant tanīyam;",
    "Ati agādh mahimā kamnīyam,",
    "chandrakānt maṇinā samnīyam...",
    "Vām bhāg upavīt virāje,",
    "bhujbandh maṇimanḍan chhāje;",
    "Uru pruthul shubh hār virāje,",
    "adhar lāl bimbāfal lāje... 1",
    "Kambu kanṭh dulrī dhrut mālam,",
    "vām prushth shubh chihna vishālam;",
    "Romyukta janmangal mūlam,",
    "shāntidāyak vināshan shulam... 2",
    "Mallikādi kusumottam mālaihi,",
    "pūjitam nij janai suprakāraihi",
    "Chandanokshit subhāl vishālam,",
    "madhya chandrakrut kānti rasālam... 3",
    "Udar shītal sadā sukhrūpam,",
    "nābhī vartul sudhājanu kūpam;",
    "Trivaḷī doshtray tāp vināsham,",
    "bhaktaranjan vināshan trāsam... 4",
    "Madhyabhāg jan kesarī tilakam,",
    "sant dharat sadāy nishankam;",
    "Jangyugma janamangalrūpam,",
    "vartulākrūtī sadāy anūpam... 5",
    "Jānuyugma maṇimarkatrūpam,",
    "dhyān dharat sadā munibhupam,",
    "Ubhaypindī sukhshānt anūpam,",
    "ghuṭī gol mud mangalrūpam... 6",
    "Charaṇ raktamati sundartāī,",
    "ūrdhvarekha nirakhī mud pāī;",
    "Tung rakta ramaṇiya nakhendu,",
    "shāntikārak sadājan īndu... 7"
  ],
