{
  "song_lyrics": [
    "Nayan ṭhare mārā tamne nihāḷī,",
    "Shyām sundar sukhkārī re;",
    "Chhel chhabīlā Shyām tamarī,",
    "bānaknī balihārī re... nay 1",
    "Rūp tamāru rasik salūṇā,",
    "jotā trupti na thāy re;",
    "Guṇsāgar tārā guṇmā lobhāṇī,",
    "tam vinā paḷ na rahevāy re... nay 2",
    "Vahālamjī tārā vachan suṇīne,",
    "mujne mohnī lāgī re;",
    "Tam vinā Shyām game nahi bīju,",
    "bhramaṇā te sarve bhāgī re... nay 3",
    "Hāralnī lakrīnī perye,",
    "nimakh na melu nyārā re;",
    "Muktānandnā Nāth manohar,",
    "param sanehī pyārā re... nay 4"
  ],
  "title": "Nayan ṭhare mārā tamne nihāḷī Shyām sundar sukhkārī re",
