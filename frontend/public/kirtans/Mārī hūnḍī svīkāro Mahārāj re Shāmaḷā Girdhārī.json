{
  "song_lyrics": [
    "Mārī hundī svīkāro Mahārāj re,",
    "Shāmaḷā Girdhārī,",
    "Mārī hundī Shāmaḷiyāne hāth re,",
    "Shāmaḷā Girdhārī...",
    "Rāṇājie raḍh karī,",
    "vaḷī Mīrā kere kāj,",
    "Jhernā pyālā mokalyā re,",
    "vahālo jhernā jāraṇhār re... Shāmaḷā 1",
    "Stambh thakī Prabhu pragaṭiyā,",
    "vaḷī dharyu Narsinhrūp,",
    "Prahlādne ugāryo re,",
    "vahāle māryo Hiranyākans bhup re... Shāmaḷā 2",
    "Rahevā nathī jhupḍu,",
    "vaḷī jamvā nathī juvār,",
    "Beṭā beṭī vaḷāviyā re,",
    "me to vaḷāvī ghar kerī nār re... Shāmaḷā 3",
    "Garath māru gopīchandan,",
    "vaḷī tulsī hemno hār,",
    "Sāchu nāṇu māre Shāmaḷo re,",
    "māre dolatmā jānjh ne pakhvāj re... Shāmaḷā 4",
    "Nathī Brāhmaṇ nathī Vāṇīyo,",
    "nathī Chāran nathī Bhāṭ,",
    "Lok kare chhe ṭhekḍī re,",
    "nathī Shāmaḷshā sheth evu nām re... Shāmaḷā 5"
  ],
