{
  "song_lyrics": [
    "Lāḍīlā māne vahālā lāgo rāj...",
    "Dekhī badan thāro rāj rasiyā,",
    "māro dukh sankaṭ dūr bhāgo... lādī 1",
    "Vā’lā lāgo chho rāj rasiyā,",
    "thāne nīrakhī neṇā ṭhare chhe,",
    "Kesariyā thāke pāchhe pāchhe,",
    "prāṇ amārā fare chhe... lādī 2",
    "Nīrakhī chalnī madhurī mīṭhī,",
    "bolnī chatur chitvanī chāru,",
    "Kahā karu bidhi ek prāṇ diyo,",
    "koṭi prāṇ le vāru... lādī 3",
    "Ati chanchal nischal hoy sevat,",
    "Shrī thārā charan vihārī,",
    "Premānandrā Nāthjī the māne,",
    "durlabh dev Murārī... lādī 4"
  ],
