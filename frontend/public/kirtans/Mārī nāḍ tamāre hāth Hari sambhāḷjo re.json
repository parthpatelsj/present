{
  "song_lyrics": [
    "Mārī nāḍ tamāre hāth,",
    "Hari sambhāḷjo re,",
    "Mujne potāno jāṇīne,",
    "Prabhu pad pāḷjo re...",
    "Pathyāpathya nathī samjātu,",
    "dukh sadaiv rahe ūbhrātu,",
    "Mane hashe shu thātu,",
    "Nāth nihāḷjo re... mārī 1",
    "Anādi āp vaidya chho sāchā,",
    "koī upāy vishe nahi kāchā,",
    "Dīvas rahyā chhe ṭānchā,",
    "veḷā vāḷjo re... mārī 2",
    "Vishveshvar shu hajī visāro,",
    "bājī hāth chhatā kā hāro,",
    "Mahā mūnjhāro māro Naṭvar,",
    "tāḷjo re... mārī 3",
    "Keshav Hari mārū shu thāshe,",
    "ghāṇ vaḷyo shu gaḍh gherāshe,",
    "Lāj tamārī jāshe,",
    "bhudhar bhāḷjo re... mārī 4"
  ],
