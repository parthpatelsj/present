{
  "song_lyrics": [
    "Āvo āvo dharma dulārā,",
    "ati Akshar tamne pyārā,",
    "Chho sant hraday vasnārā...",
    "Tame akaḷit antarjāmī,",
    "mam hraday vasyā bahunāmī,",
    "Sukh shānti have hu pāmī,",
    "O Swāmī tame chho amārā,",
    "chho sant hraday vasnārā... āvo 1",
    "Tame Dharmatanuj tandhārī,",
    "tav mūrti mangalkārī,",
    "Avatār taṇā avatārī,",
    "Nitya navā neh rachnārā,",
    "chho sant hraday vasnārā... āvo 2",
    "Nen katāksh jyārthī bhedī,",
    "traṇ deh vindhāyā tethī,",
    "Ek tār banī chhu ethī,",
    "O Swāmī tame chho amārā,",
    "chho sant hraday vasnārā... āvo 3",
    "Tame vīsaro nahi visāryā,",
    "evā neṇ katāksho māryā,",
    "Kīṭ bhamrī sam me dhāryā,",
    "Motīnā jīvan ādhārā,",
    "chho sant hraday vasnārā... āvo 4"
  ],
