{
  "song_lyrics": [
    "Maḷyā mune Shāstrījī Mahārāj,",
    "Jīvtarnī e dhanya ghaḍī,",
    "Mūrti antar māhī jaḍī...",
    "Samvat ogaṇīsso chhannumā,",
    "Kārtak sudi dasmī,",
    "Dhanya milan e guruharinu,",
    "Poḷ Āmbalīvāḷī... maḷyā mune 1",
    "Hasta pasārī tāv utārī,",
    "Prīt pūravnī jatāvī,",
    "Paṇ hrudiyāmā jādu kartā,",
    "Harimūrti padhrāvī... maḷyā mune 2",
    "Sant swarūpe Shrījī maḷiyā,",
    "Virahvednā bhāgī,",
    "Thaī jāvu Shāstrīmay māre,",
    "Raṭnā evī lāgī... maḷyā mune 3"
  ],
