{
  "song_lyrics": [
    "Prabhu ek nām terā sukhkārī,",
    "Ye duniyā hai dukhkārī...",
    "Garbhvās me ūlṭe mukh se,",
    "nau-das mās gujārī,",
    "Bāhir āy paḍā pruthvī par,",
    "māyā lipatī tumhārī... Prabhu 1",
    "Bālpaṇe me parādhīn nit,",
    "mātā god khīlārī,",
    "Sukhḍukh sabhī vyāpat tanme,",
    "bachan sake na uchchārī... Prabhu 2",
    "Joban me nit kām satāve,",
    "moh liyo man nārī,",
    "Bāl bachcho ke pālan ke hit,",
    "parghar dās bhikhārī... Prabhu 3",
    "Vrūddhpaṇe me rog lage sab,",
    "kāyā nīrbal hārī,",
    "Brahmānand bhajan bin tumhare,",
    "janma maraṇ bhay bhārī... Prabhu 4"
  ],
