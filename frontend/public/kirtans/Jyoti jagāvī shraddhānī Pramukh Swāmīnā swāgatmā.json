{
  "song_lyrics": [
    "Jyoti jagāvī shraddhānī,",
    "Pramukh Swāmīnā swāgatmā,",
    "Satkār karu hu laḷī naman karu...",
    "Saunā haiyā pulkit thāye,",
    "man mandirmā gīt gunjāye;",
    "Karī darshan Swāmī āpnā,",
    "dhanya thāy shubh kāmnā... satkār 1",
    "Rāg rangmā vite janmāro,",
    "ekaj chhe have tāro sahāro;",
    "Mūrti vasī tav hradaye Swāmī,",
    "āshish dejo hu chhu anugāmī... satkār 2",
    "Brahmaswarūpe āp chho sāthmā,",
    "divya smaraṇ chhe ek ja nāmmā;",
    "Pankhīnī pānkh thaī uḍave ābhmā,",
    "sukh samāyu tav charaṇomā... satkār 3"
  ],
