{
  "song_lyrics": [
    "Mongho manushya deh farī farīne,",
    "maḷe nahi vāramvār,",
    "bhāī tu bhajī lene kirtār...",
    "Jūṭhī chhe kāyā ne jūṭhī chhe māyā,",
    "jūṭho kuṭumb parīvār,",
    "Rājā Gopīchand ane Bhartharī,",
    "chhoḍī gayā gharbār... 1",
    "Kām krodh mad lobh mohmā,",
    "gnāne juo nathī sār,",
    "Ā avsar jo chukī gayā to,",
    "khāsho Jamnā mār... 2",
    "Lākho gayā ne tu paṇ javāno,",
    "mūrakh manmā vichār,",
    "Sāchu kahu chhu chhatā jūṭhu māne to,",
    "muvā kuṭumbne sambhār... 3",
    "Sad chalaṇ sadgurunī sevā,",
    "sajjannā shaṇgār,",
    "Dās satār kahe kar joḍī,",
    "Hari bhajī utaro pār... 4"
  ],
