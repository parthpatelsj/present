{
  "song_lyrics": [
    "Maḷyo manushya deh monghā mūlno,",
    "Farī farī na āve jo eh... samajvu sānmā 1",
    "Jene dev vānchhe ghaṇu dalmā,",
    "toy pāme nahi tan teh... sam 2",
    "Nartan te nautam nāv chhe,",
    "tarvāne samudra sansār... sam 3",
    "Temā maḷe sadguru mālmī,",
    "tyāre utāre paḷmā pār... sam 4",
    "Evā avsar avar āve nahi,",
    "sarkhe sarkho maḷyo chhe samāj... sam 5",
    "Sadgurujīne shīsh ā arpīe,",
    "karī laīe jo āpṇu kāj... sam 6",
    "Samā vinā te arth sare nahī,",
    "rahīe samāsme sāvdhān... sam 7",
    "Mān mūkī maḷīe mahantne,",
    "āpe sadgurujī nij gnān... sam 8",
    "Bhānge khoṭ moṭī khoyā dannī,",
    "vāge jīt taṇā jāngī ḍhol... sam 9",
    "Shobhe jīvit tenu sansārmā,",
    "māṇe ānand ati atol... sam 10",
    "Thaī magan moj ghaṇī māṇīe,",
    "sāchā sadguru santne sang... sam 11",
    "Maḷi Nishkuḷānand mahantne,",
    "līje akhanḍ ānand ang... sam 12"
  ],
  "title": "Maḷyo manushya deh monghā mūlno",
