{
  "song_lyrics": [
    "Raho ne Swāmī āj amārā,",
    "hraday mandirmā rāj...",
    "Mangamtu mūkīne māru,",
    "Gamtu tamāru kāj;",
    "Karu nitya e prārthanā mārī,",
    "Faḷjo āj ne āj re ho... 1",
    "Kāmādi doshothī pīḍātā ame,",
    "Farīe chhīe jagmāy;",
    "Nāsh karo e shatru amārā,",
    "Dās tamārā rahevāy re ho... 2",
    "Vishay vaheṇmā taṇātā ame,",
    "Rāchtā jag rang māy;",
    "Tārī lyo e moh māyāthī,",
    "Tāraṇhār chho āp re ho... 3",
    "Sanāth ame to Nāth tame chho,",
    "Chintā paḷ paḷ karnār;",
    "Bhakti karīe sadāy tamārī,",
    "Eṭlu amthī thanār re ho... 4"
  ],
