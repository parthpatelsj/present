{
  "song_lyrics": [
    "Jamo jamo re mārā Jīvan jugate,",
    "Bhojaniyā ras bhariyā re;",
    "Pāk shāk tam sāru Prītam,",
    "Koḍe koḍe kariyā re... jamo 1",
    "Taḷiyā gaḷiyā tājā tātā,",
    "Kanak thāḷmā bhariyā re;",
    "Ārogo mārā Nāth alaukik,",
    "Ghrut jhājhā ghebariyā re... jamo 2",
    "Kaḍhī vaḍi kārelā kāju,",
    "Rāitaṇā dahīthariyā re;",
    "Joīe to uparthī lejo,",
    "Mīṭhu jīrū ne mariyā re... jamo 3",
    "Brahmānandnā Nāth shirāvyā,",
    "Dūdhbhāt sākariyā re;",
    "Chaḷu karyu Hari trupta thaīne,",
    "Nīrakhī lochan ṭhariyā re... jamo 4"
  ],
  "title": "Jamo jamo re mārā Jīvan jugate",
