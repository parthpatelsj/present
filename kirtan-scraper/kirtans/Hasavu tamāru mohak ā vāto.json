{
  "song_lyrics": [
    "Hasavu tamāru mohak ā vāto,",
    "Chahero dīpe chhe malakāto chāndo,",
    "Nazaru jyā paḍatī bhāgya ūghaḍatī,",
    "Maru bhūmīe varṣhānī bharatī,",
    "Hasavu tamāru... 1",
    "Kṛupāe grahyo chhe tame ja hāth,",
    "Kem e chhūṭe nahī tāro sangāth,",
    "Tuj pagale pagale dorī ja leje,",
    "Naiyā ḍūbe nahī nāvik joje,",
    "Hasavu tamāru... 2",
    "Zakhamo ghaṇā ā ṛudaye paḍyā chhe,",
    "Kharā banī mujne khoṭā jaḍyā chhe,",
    "Bas have tu ek ja raheje,",
    "Tuj vīṇ bīju kāī na deje,",
    "Hasavu tamāru.... 3",
    "Hu chhu agnānī pāmar pūro,",
    "Doṣhe swabhāve jīvane adhūro,",
    "Ṭānkaṇe ghāv shilpī tu deje,",
    "Tārā samo mune ākār deje,",
    "Hasavu tamāru... 4",
    "Shruti na jāṇu smṛuti shu vakhāṇu,",
    "Sūje na mujne sāchu ke ṭāṇu,",
    "Shabda sakalno sār tu kaheje,",
    "Akṣhar tārī oḷakh tu deje,",
    "Hasavu tamāru... 5",
    "Bājī hāryo chhu mannā sangrāme,",
    "Taraṇe dhrūju chhu māyānī sāme,",
    "Kunḍaḷ kavach tu paherāvī deje,",
    "Hāru have nā sārathī joje,",
    "Hasavu tamāru... 6",
    "Rājī karu hu tune ek hete,",
    "Āyakhu māru tav pāv vīte,",
    "Dayā grahīne nīj dhām leje,",
    "Sevā Shrījīnī sharaṇe tu deje,",
    "Hasavu tamāru... 7"
  ],
  "title": "Hasavu tamāru mohak ā vāto",
