{
  "song_lyrics": [
    "Manmohan sundarvar mārā,",
    "Avguṇiyā visrāvā re;",
    "Adhamodhārak patitjan pāvan,",
    "Birad tamāru chahāvā re...",
    "Sahu dekhtā Shyāmaḷīyā tame,",
    "Māḍī thayā chho māvā re;",
    "Chhorunī chintā rākhine,",
    "Mā desho mūnjhāvā re... man 1",
    "Bāḷak hoy te jem tem bole,",
    "Līe kuṭāne khāvā re;",
    "Janunī dosh na lekhe jāṇe,",
    "Aṇsamju hoy āvā re... man 2",
    "Sheraḍīe ramtā athḍātā,",
    "Chhorā rākhe dāvā re;",
    "Sabaḷ thaī koī līe chūnṭīo,",
    "To mātā jāy melāvā re... man 3",
    "Mangamtu sarve melīne,",
    "Ek tamne rījhāvā re;",
    "Brahmānand kahe man karma vachane,",
    "Guṇ tamārā gāvā re... man 4"
  ],
