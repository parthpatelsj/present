{
  "song_lyrics": [
    "Āvo āvo Shrījī Swāmī āvo,",
    "āvīne māre mandir padhāro;",
    "Mandir padhāro māro janam sudhāro...",
    "Jarkasī jāmo paherī,",
    "sundar sūrvāḷ paherī;",
    "Pāvmā hu mojḍī paherāvu,",
    "āvīne māre mandir padhāro... āvo 1",
    "Māthe mugaṭ mūkī,",
    "motī torā rahyā jhukī;",
    "Bhālmā hu chāndlo karāvu,",
    "āvīne māre mandir padhāro... āvo 2",
    "Kānmā kunḍaḷ paherī,",
    "kanṭhmā māḷā paherī;",
    "Gaḷāmā hār hu paherāvu,",
    "āvīne māre mandir padhāro... āvo 3",
    "Sahajānand āvo,",
    "tame Guṇātīt āvo;",
    "Bhagatji Mahārāj āvo,",
    "tame Shāstrījī Mahārāj āvo,",
    "Yogī Mahārāj āvo,",
    "tame Pramukh Swāmī āvo,",
    "Mahant Swāmī āvo,",
    "tame Aksharmukto āvo;",
    "Preme karī ārtī utāru,",
    "āvīne māre mandir padhāro... āvo 4"
  ],
  "title": "Āvo āvo Shrījī Swāmī āvo",
