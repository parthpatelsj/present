  "song_lyrics": [
    "Pyāre darshan dījo āya,",
    "Tum bin rahyo na jāy... °ṭek",
    "Jal bin kamal chanda bin rajnī,",
    "Aise tum dekhe bin sajnī;",
    "Ākul vyākul firu rai din,",
    "Virah kalejā khāy... Tum° 1",
    "Divas na bhūkh nīnd nahī rainā,",
    "Mukhsu kathat na āve bainā;",
    "Kahā kahu kuchh kahat na āve,",
    "Milakar tapat buzāy... Tum° 2",
    "Kyo tarsāvo antaryāmī,",
    "Āy milo kirpākar Swāmī;",
    "Mīrā dāsī janam janam kī,",
    "Paḍī tumharo pāy... Tum° 3"
  ],
