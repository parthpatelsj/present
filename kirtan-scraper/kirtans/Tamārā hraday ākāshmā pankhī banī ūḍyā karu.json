{
  "song_lyrics": [
    "Tamārā hraday ākāshmā,",
    "pankhī banī ūḍyā karu;",
    "Ānkhonī pāpaṇomā,",
    "amī banī jharyā karu...",
    "Tamārī muskāne jīvan māru,",
    "enī smrutie jīvyā karu;",
    "Hradaynā dvārethī tamne,",
    "joyā karu, joyā karu... ānkhonī 1",
    "Tu chhe māro jīvan sahāro,",
    "tu chhe māro sāgar kināro;",
    "E sāgarnī mīṭhī lahere lahere,",
    "sandhyā banī ḍhaḷyā karu... ānkhonī 2"
  ],
