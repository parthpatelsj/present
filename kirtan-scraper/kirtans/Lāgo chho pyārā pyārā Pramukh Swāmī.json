{
  "song_lyrics": [
    "Lāgo chho pyārā pyārā Pramukh Swāmī,",
    "lāgo chho pyārā pyārā,",
    "Jīvan prāṇ amārā Pramukh Swāmī, lāgo chho...",
    "Koṭi sūrajnu tej bharyu āpmā,",
    "Antarnu tam ṭāḷnārā... Pramukh Swāmī 1",
    "Vaḍvānaḷ sam ati baḷvantā,",
    "Brahma prakāsh relnārā... Pramukh Swāmī 2",
    "Anant chandranī shītaḷtā āpmā,",
    "Taptā haiyā ne ṭhārnārā... Pramukh Swāmī 3",
    "Gārūdī nā gārūdī āp chho,",
    "Maṇidhar ḍolāvnārā... Pramukh Swāmī 4",
    "Sarve sāmarthī aīshvarya dābī,",
    "Namra bhāve vartnārā... Pramukh Swāmī 5",
    "Swāmī Shrījīnā akhanḍ dhārak,",
    "Sādhutā shobhāvnārā... Pramukh Swāmī 6",
    "Paropkārī sant guruhari,",
    "Vanmāḷīdāsnā vahālā... Pramukh Swāmī 7"
  ],
