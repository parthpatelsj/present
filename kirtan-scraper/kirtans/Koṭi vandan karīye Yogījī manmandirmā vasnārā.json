{
  "song_lyrics": [
    "Koṭi vandan karīye Yogījī,",
    "manmandirmā vasnārā...",
    "Pyārā gaṇjo potānā amne,",
    "sopī jīvan naiyā tamne;",
    "Bhavjaḷthī tārone Swāmī,",
    "āp ja chho tāraṇhārā... koṭi 1",
    "Jevā tevā toye bāḷ tamārā,",
    "dosho māf karīne amārā;",
    "Charaṇomā rākhī lyo Swāmī,",
    "kashṭ taṇā chho harnārā... koṭi 2",
    "Gnān taṇā Swāmī āp chho dariyā,",
    "prem jaḷe saunā haiyā bhariyā;",
    "Snehbhīnī ānkhothī Swāmī,",
    "varsāvo chho amīdhārā... koṭi 3",
    "Sau par me’r karo chho Swāmī,",
    "mannā koḍ puro chho Swāmī,",
    "Pragaṭ brahma Ghanshyām chho Swāmī,",
    "āp amārā rakhvāḷā... koṭi 4"
  ],
