{
  "song_lyrics": [
    "Ame sau Shrījīnā santān,",
    "amārā Shrījī chhe Bhagwān...",
    "Shrījī amārā māt pitā ne,",
    "Shrījī sukh bhanḍār,",
    "Shrījī māṭe shir amārā,",
    "sadaīv chhe taiyār... ame sau 1",
    "Shrījī kāraṇ tan dhan deshu,",
    "leshu nām hammesh,",
    "Kshaṇbhangur sukh sampat māṭe,",
    "chaḷshu nahi lavlesh... ame sau 2",
    "Nishdin dhyān Shrījīnu dharīe,",
    "nahi jagni darkār,",
    "Akhanḍ sukhḍā laīne karīe,",
    "sārthak ā avatār... ame sau 3",
    "Shrījī sharaṇe shir namāvī,",
    "vandīe vāramvār,",
    "Krupāsindhunī krupā yāchīe,",
    "utāro bhavpār... ame sau 4"
  ],
